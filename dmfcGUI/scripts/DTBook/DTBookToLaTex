<?xml version="1.0" encoding="UTF-8"?>
<taskScript version="1.0">
	<name>DTBook to LaTeX</name>
	<description>Converts a DTBook 2005 document to LaTeX 2e</description>
	
	<property name="input" value="FIXME" type="application/x-dtbook+xml"/>
	<property name="outputPath" value="FIXME" type="application/x-latex"/>
	
	<task name="se_tpb_dtbook2latex" interactive="false">

		<parameter>
			<name>xml</name>
			<value>${input}</value>
		</parameter>
					
		<parameter id="id1">
			<name>out</name>
			<value>${outputPath}</value>
		</parameter>		
				
	</task>

</taskScript>
