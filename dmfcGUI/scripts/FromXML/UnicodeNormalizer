<?xml version="1.0" encoding="UTF-8"?>
<taskScript version="1.0">
	<name>Unicode Normalizer</name>
	<description>Performs Unicode normalization on all XML files in a fileset</description>
	
	<property name="input" value="FIXME" type="application/xml"/>
	<property name="outputPath" value="FIXME" type="application/xhtml+xml"/>
	
	
	<task name="int_daisy_unicodeNormalizer" interactive="true">

		<parameter>
			<name>inXML</name>
			<value>${input}</value>
		</parameter>

		<parameter id="id1">
			<name>outDir</name>
			<value>${outputPath}</value>
		</parameter>
		
	</task>
	
</taskScript>
