<?xml version="1.0" encoding="UTF-8"?>
<taskScript version="1.0">
	<name>ErrorScout DAISY/NISO</name>
	<description>Performs error scouting on a DAISY/NISO DTB.</description>
	
	<property name="input" value="FIXME" type="application/xml"/>
	
	<task name="se_tpb_dtbErrorScout" interactive="false">

		<parameter>
			<name>manifest</name>
			<value>${input}</value>
		</parameter>
		
		<parameter>
			<name>scoutLevel</name>
			<value>MAXED</value>
		</parameter>
		
		<parameter>
			<name>filesetType</name>
			<value>Z3986</value>
		</parameter>
						
	</task>

</taskScript>
