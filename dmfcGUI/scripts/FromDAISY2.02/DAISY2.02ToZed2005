<taskScript version="1.0">
	<name>Daisy202 to Zed2005</name>
	<description>Converts a Daisy 2.02 DTB to a Zed 2005 DTB</description>
	
	<property name="input" value="FIXME" type="application/x-dtbd202ncc+xml"/>
	<property name="outputPath" value="FIXME" type="application/x-filesystemDirectory"/>
	
	<task name="int_daisy_daisy2022Zed2005" interactive="false">
	
		<parameter>
			<name>ncc</name>
			<value>${input}</value>
		</parameter>
					
		<parameter id="id1">
			<name>outDir</name>
			<value>${outputPath}</value>
		</parameter>				
		
	</task>

</taskScript>
