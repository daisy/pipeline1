<?xml version="1.0" encoding="UTF-8"?>
<taskScript version="1.0">
	<name>WordML to DTBook</name>
	<description>Converts a WordML document to DTBook 2005-1</description>
	
	<property name="input" value="FIXME" type="application/xml"/>
	<property name="outputPath" value="FIXME" type="application/x-dtbook+xml"/>
	
	
	<task name="se_tpb_wordml2dtbook" interactive="true">

		<parameter>
			<name>xml</name>
			<value>${input}</value>
		</parameter>

		<parameter id="id1">
			<name>out</name>
			<value>${outputPath}</value>
		</parameter>
		
	</task>
	
</taskScript>
