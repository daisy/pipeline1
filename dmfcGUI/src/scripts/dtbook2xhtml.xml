<taskScript version="1.0">
	<name>DTBook2XHTML</name>
	<description>This script converts a DTBook 2005 document to XHTML 1.0</description>
	
	<task name="uk_rnib_dtbook2xhtml" interactive="false">

		<parameter>
			<name>xml</name>
			<value>d:/books/tb50.xml</value>
		</parameter>
					
		<parameter id="id1">
			<name>out</name>
			<value>d:/books/test.html</value>
		</parameter>		
				
	</task>

</taskScript>
